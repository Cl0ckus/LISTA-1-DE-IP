#include <stdio.h>
#include <string.h>

void strsub(char *str1[200], char *str2[200], int i){

    int j, lenStr,k=0;
    lenStr= i+1+strlen(str2);

    for(j=i; j<lenStr; j++){

       str1[j]=str2[k];
        

        k++;

    }  

}

int main(){

    int iM;
    char strM1[200], strM2[200];

    printf("Type a string:\n");
    scanf(" %[^\n]s", strM1[200]);
    printf("Type another string:\n");
    scanf(" %[^\n]s", strM2[200]);
    printf("Type an integer");
    scanf("%d", iM);

    printf("%s", strsub(strM1, strM2, iM));
    

    return 0;
}

